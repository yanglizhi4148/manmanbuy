html {
  background-color: #f2f2f2;
  font-family: "Helvetica Neue", Helvetica, STHeiTi, sans-serif !important;
}
.layout {
  width: 100%;
  height: 100%;
  min-width: 320px;
  max-width: 640px;
  margin: 0 auto;
}
/*coupon top*/
.coupon_top {
  background-color: #f4483e;
  width: 100%;
  margin: 0;
  padding: 0;
  position: relative;
}
.coupon_top > .coupon_left,
.coupon_top .coupon_right {
  width: 50px;
  height: 44px;
  position: absolute;
  color: #fff;
  top: 0;
  text-align: center;
}
.coupon_top > .coupon_left > .glyphicon,
.coupon_top .coupon_right > .glyphicon {
  line-height: 44px;
  font-size: 19px;
  font-weight: normal;
}
.coupon_top > .coupon_left > .glyphicon-download-alt,
.coupon_top .coupon_right > .glyphicon-download-alt {
  position: absolute;
  top: -8px;
  left: 16px;
  font-size: 16px;
}
.coupon_top .coupon_left {
  left: 0;
}
.coupon_top .coupon_right {
  right: 0;
}
.coupon_top .coupon_right > b {
  font-size: 10px;
  margin-top: 24px;
  margin-right: 8px;
  font-weight: normal;
}
.coupon_top h3 {
  height: 100%;
  font-size: 20px;
  height: 44px;
  padding: 0;
  margin: 0;
  line-height: 44px;
  text-align: center;
  color: #fff;
  margin: 0 50px;
}
/*coupon img */
.coupon_img {
  padding: 20px 0 0 0;
  width: 100%;
}
.coupon_img #coupon_img_model {
  width: 100%;
  margin: 0;
  /* div:nth-of-type(-2){
       margin: 10px 0;
     }*/
}
.coupon_img #coupon_img_model div {
  margin-bottom: 15px;
  padding: 0 ;
  padding-left: 5px;
}
.coupon_img #coupon_img_model div a {
  display: block;
  width: 100%;
  border: 1px solid #ccc;
  padding-top: 5px;
}
.coupon_img #coupon_img_model div a > img {
  display: block;
  width: 50px;
  height: 50px;
  margin: 0 auto;
  margin-top: 8px;
}
.coupon_img #coupon_img_model div a > p {
  text-align: center;
  margin-top: 3px ;
}
/*coupon back*/
.coupon_back {
  height: 38px;
  line-height: 38px;
  margin: 30px 0;
}
.coupon_back > div {
  padding-left: 15px;
  font-size: 16px;
  background-color: #ebebeb;
  border-top: 1px solid #ddd;
  border-bottom: 1px solid #ddd;
}
.coupon_back > div > a {
  color: #000;
}
/* coupon bottom*/
.coupon_bottom {
  width: 100%;
  background-color: #efefef;
}
.coupon_bottom > div {
  border-top: 1px solid #c4c4c4;
  border-bottom: 1px solid #c4c4c4;
  height: 34px;
  width: 100%;
  line-height: 34px;
}
.coupon_bottom > div > a {
  display: inline-block;
  /* width: 33%;*/
  height: 34px;
  line-height: 34px;
  text-align: center;
  font-size: 16px;
  color: #000;
  border-left: 1px solid #c4c4c4;
}
.coupon_bottom > div > a > i {
  font-weight: 700;
  color: #5e6e6e;
  margin-right: 5px;
}
.coupon_bottom > div > a > div {
  display: inline-block;
  color: #666;
}
.coupon_bottom > div > a:nth-child(1) {
  border-left: none;
}
.coupon_bottom > p {
  text-align: center;
  padding-top: 18px;
  margin: 0;
}
.coupon_bottom > p > a {
  color: #000;
}
.coupon_bottom > p > .bottom {
  display: block;
  color: #666;
  padding-bottom: 45px;
}
.coupon_bottom > p > .bijia {
  color: #666;
}
.coupon_bottom > p > a:first-of-type {
  color: #f4483e;
}
/*coupon product*/
.coupon_product {
  width: 100%;
}
.coupon_product > p {
  text-align: center;
  color: #999;
  margin-top: 5px;
  font-size: 12px;
}
.coupon_product .coupon_product_img {
  width: 100%;
  padding: 10px 0 15px 10px;
  color: #333;
  font-size: 14px;
  border-bottom: 1px solid #ccc;
}
.coupon_product .coupon_product_img > a {
  display: block;
  position: relative;
  background: url("../../img/arrow_right.svg") right center no-repeat;
  background-size: 20px 20px;
}
.coupon_product .coupon_product_img > a > img {
  float: left;
  width: 100px;
  height: 80px;
}
.coupon_product .coupon_product_img > a > .coupon_des {
  padding-left: 110px;
  width: 100%;
}
.coupon_product .coupon_product_img > a > .coupon_des > div {
  line-height: 22px;
}
.coupon_product .coupon_product_img > a > .coupon_des > .coupon_product_price {
  margin-top: 3px;
}
.coupon_product .coupon_product_img > a > .coupon_des div:last-of-type {
  font-size: 11px;
  margin-top: 15px;
  color: #f4483e;
}
/*coupon   mask*/
#coupon_mask {
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  display: none;
}
#coupon_mask > .coupon_mask_box {
  float: left;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.1);
  position: relative;
}
#coupon_mask > .coupon_mask_box > img {
  width: 200px;
  height: 160px;
  position: absolute;
  left: 50%;
  top: 50%;
  background-color: pink;
  transform: translate(-50%, -50%);
  -webkit-transform: translate(-50%, -50%);
  -o-transform: translate(-50%, -50%);
}
/*coupon arrow*/
.coupon_arrow {
  width: 100%;
  height: 100%;
  display: none;
}
.coupon_arrow > div {
  width: 43px;
  height: 58px;
  position: fixed;
  top: 50%;
  background-color: #0f0f0f;
  opacity: 0.3;
  font-size: 38px;
  font-weight: bold;
  color: #fff;
  line-height: 58px;
  text-align: center;
  border-radius: 8px;
  -ms-border-radius: 8px;
  -webkit-border-radius: 8px;
  -o-border-radius: 8px;
  -moz-border-radius: 8px;
}
.coupon_arrow > .coupon_arrow_left {
  left: 30px;
}
.coupon_arrow > .coupon_arrow_right {
  right: 30px;
}
@keyframes moveLeft {
  /*from�൱��0%*/
  /*�ؼ�֡*/
  0% {
    margin-left: 40px;
  }
  100% {
    margin-left: 0px;
  }
}
